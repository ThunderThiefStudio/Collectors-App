{
    "expo":  {
                 "name":  "Collector\u0027s App",
                 "slug":  "collectors-app",
                 "version":  "1.0.0",
                 "orientation":  "portrait",
                 "icon":  "./assets/images/icon.png",
                 "scheme":  "myapp",
                 "userInterfaceStyle":  "automatic",
                 "newArchEnabled":  true,
